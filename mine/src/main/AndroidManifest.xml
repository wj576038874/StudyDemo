<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joyrun.mine">

    <application
        android:name=".MineApplication"
        android:allowBackup="true"
        android:icon="@mipmap/mine_ic_launcher"
        android:label="@string/mine_app_name"
        android:roundIcon="@mipmap/mine_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/mine_AppTheme">
        <activity android:name=".MineActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".UserActivity" />
    </application>

</manifest>