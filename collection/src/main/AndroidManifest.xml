<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joyrun.collection">

    <application
        android:name=".CollectionApplication"
        android:allowBackup="true"
        android:icon="@mipmap/collection_ic_launcher"
        android:label="@string/collection_app_name"
        android:roundIcon="@mipmap/collection_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/collection_AppTheme">
        <activity android:name=".CollectionActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>