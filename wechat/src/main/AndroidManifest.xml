<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joyrun.wechat">


    <application
        android:name=".WechatApplication"
        android:allowBackup="true"
        android:icon="@mipmap/wechat_ic_launcher"
        android:label="@string/wechat_app_name"
        android:roundIcon="@mipmap/wechat_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/wechat_AppTheme">
        <activity android:name=".WechatActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>